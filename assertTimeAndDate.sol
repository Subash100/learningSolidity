pragma solidity >=0.7.0 <0.9.0;
contract learnDate
{
    //seconds,minutes,hours,days and weeks

    function time() public
    {
        assert(1 minutes==60 seconds);
        assert(1 hours==60 minutes);
        assert(24 hours==1 days);
        assert(7 days==1 weeks);
    }
}